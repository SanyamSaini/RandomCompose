<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RandomComposable"
        tools:targetApi="31">
        <activity
            android:name=".TopAppBarActivity"
            android:exported="false"
            android:label="@string/title_activity_top_app_bar"
            android:theme="@style/Theme.RandomComposable" />
        <activity
            android:name=".LearnStateActivity"
            android:exported="false"
            android:label="@string/title_activity_learn_state"
            android:theme="@style/Theme.RandomComposable" />
        <activity
            android:name=".AlignmentArrangementActivity"
            android:exported="false"
            android:label="@string/title_activity_alignment_arrangement"
            android:theme="@style/Theme.RandomComposable" />
        <activity
            android:name=".RCBActivity"
            android:exported="false"
            android:label="@string/title_activity_rcbactivity"
            android:theme="@style/Theme.RandomComposable" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RandomComposable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>